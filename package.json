{
  "name": "eslint-config-yinm",
  "version": "0.0.4",
  "description": "ESLint config for @yinm",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/index.js"
    },
    "./react": {
      "types": "./src/react.d.ts",
      "default": "./src/react.js"
    },
    "./typescript": {
      "types": "./src/typescript.d.ts",
      "default": "./src/typescript.js"
    }
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "src",
    "!src/external-package-typings"
  ],
  "scripts": {
    "test": "eslint -c test/eslint.config.js test/cases"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "author": "Yusuke Iinuma <yinm.cminor9@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "eslint": ">= 9"
  },
  "devDependencies": {
    "eslint": "^9.5.0",
    "typescript": "^5.5.2"
  },
  "packageManager": "pnpm@9.12.2",
  "dependencies": {
    "eslint-plugin-import-x": "^4.0.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-unused-imports": "^4.0.0",
    "typescript-eslint": "^8.0.0"
  }
}
