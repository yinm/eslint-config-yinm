{
  "name": "eslint-config-yinm",
  "version": "0.0.6",
  "description": "ESLint config for @yinm",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/index.js"
    },
    "./react": {
      "types": "./src/react.d.ts",
      "default": "./src/react.js"
    },
    "./typescript": {
      "types": "./src/typescript.d.ts",
      "default": "./src/typescript.js"
    }
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "src",
    "!src/external-package-typings"
  ],
  "scripts": {
    "test": "eslint -c test/eslint.config.js test/cases"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "author": "Yusuke Iinuma <yinm.cminor9@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "eslint": ">= 9"
  },
  "devDependencies": {
    "eslint": "^9.17.0",
    "typescript": "^5.7.2"
  },
  "packageManager": "pnpm@10.6.1",
  "dependencies": {
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "typescript-eslint": "^8.19.1"
  }
}
